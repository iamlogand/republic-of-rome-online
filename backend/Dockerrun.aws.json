{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "app",
      "image": "iamlogand/republic-of-rome-online-backend:rorsite-build-${TAG}",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        },
        {
          "hostPort": 443,
          "containerPort": 443
        }
      ]
    },
    {
      "name": "redis",
      "image": "redis:latest",
      "essential": true,
      "memory": 128
    }
  ]
}
